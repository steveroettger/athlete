<% provide(:title, 'All users') %>
<section id="user-index">
  <h1>All users</h1>

  <%= will_paginate %>
  <ul class="users">
    <% @users.each do |user| %>
      <li>
        <%= link_to image_tag(user.profile_image), user %>
        <%= link_to user.name, user %>
      </li>
    <% end %>
  </ul>
  <%= will_paginate %>

