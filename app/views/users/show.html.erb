<% provide(:title, current_user.name) %>
<h1><%= current_user.name %></h1>
<table class="table table-striped table-bordered">
	<tbody>
		<tr>
			<td>User Full Name</td>
			<td><%= @user.name %></td>
		</tr>
		
		<tr>
			<td>User Location</td>
			<td><%= @user.location %></td>
		</tr>
		<tr>
			<td>FB Username URL</td>
			<td><%= @user.fb_link %></td>
		</tr>
		
	</tbody>
</table><!--/ .table .table-striped .table-bordered--> 